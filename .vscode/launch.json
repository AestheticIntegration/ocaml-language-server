{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "port": 6004,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/bin/**/*.js",
        "${workspaceRoot}/lib/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/bin/server/index.js",
      "cwd": "${workspaceRoot}"
    }
  ]
}
